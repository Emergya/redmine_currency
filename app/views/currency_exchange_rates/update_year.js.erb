<% if @flash == 'notice' %>
$('tr[data-year="<%=@year.to_s%>"]').html("<%= escape_javascript(render :partial => 'exchange_rate_row', :locals => {:year => @year, :exchange_rates => @exchange_rates}) %>");
<% end %>

$('.flash').remove();
div = "<div class='flash <%=@flash%>' id='flash_<%=@flash%>'><%=@message%></div>"
$('#content').prepend(div);